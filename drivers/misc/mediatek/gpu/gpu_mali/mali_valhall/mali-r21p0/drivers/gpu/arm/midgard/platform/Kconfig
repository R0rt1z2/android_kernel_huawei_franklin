config MALI_HUAWEI_CL_BOOST
	bool "Enable HUAWEI mali GPU CL Boost"
	default n
	help
	enable MALI GPU CL boost.

config HW_GPU_CORE_HOTPLUG
	bool "Enable gpu core hotplug if high temperature scenario"
	depends on MACH_MT6885
	default n
	help
	  enable some cores shutdown if the thermal need reduce gpu power consumption

# Add your platform specific Kconfig file here
#
# "drivers/gpu/arm/midgard/platform/xxx/Kconfig"
#
# Where xxx is the platform name is the name set in MALI_PLATFORM_NAME
#


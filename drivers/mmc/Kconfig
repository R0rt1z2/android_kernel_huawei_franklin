#
# MMC subsystem configuration
#

menuconfig MMC
	tristate "MMC/SD/SDIO card support"
	depends on HAS_IOMEM
	help
	  This selects MultiMediaCard, Secure Digital and Secure
	  Digital I/O support.

	  If you want MMC/SD/SDIO support, you should say Y here and
	  also to your specific host controller driver.
config HUAWEI_SDCARD_DSM
	bool "Listen sdcard kernel err"
	depends on MMC != n
	default n
	help
	   use huawei sdcard log radar

config HUAWEI_EMMC_DSM
	bool "Listen eMMC kernel err"
	depends on MMC != n
	default n
	help
	   use huawei emmc log radar

config MMC_DW_MUX_SDSIM
	tristate "MUX SDSIM support"
	default n
	help
	  This selects support for MMC_DW MUX_SDSIM.

config MSDC_SDSIM_DIRECT_CONNECT
	tristate "MUX SDSIM DIRECT CONNECT support"
	default n
	depends on MMC_DW_MUX_SDSIM
	help
	  This selects support for MSDC_SDSIM_DIRECT_CONNECT.

if MMC

source "drivers/mmc/core/Kconfig"

source "drivers/mmc/host/Kconfig"

endif # MMC

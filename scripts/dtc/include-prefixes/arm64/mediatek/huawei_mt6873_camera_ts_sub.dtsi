	pinctrl-7 = <&cam0_vcama_en_0>;
	pinctrl-8 = <&cam0_vcama_en_1>;
	pinctrl-9 = <&cam0_vcamaf_en_0>;
	pinctrl-10 = <&cam0_vcamaf_en_1>;
	pinctrl-11 = <&cam0_mclk_off>;
	pinctrl-12 = <&cam0_mclk_2ma>;
	pinctrl-13 = <&cam0_mclk_4ma>;

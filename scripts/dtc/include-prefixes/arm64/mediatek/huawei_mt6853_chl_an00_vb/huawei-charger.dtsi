#include "../huawei_charger/mt6853_chl_40w_huawei_charger.dtsi"
/* 2*sm5450 */

&pio {
	sm5450_main_irq_pins_default: sm5450_main_irq_pins_default {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO45__FUNC_GPIO45>;
			slew-rate = <0>;
			bias-pull-up;
		};
	};
	sm5450_aux_irq_pins_default: sm5450_aux_irq_pins_default {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO43__FUNC_GPIO43>;
			slew-rate = <0>;
			bias-pull-up;
		};
	};
};

&i2c7 {
	sm5450@6F {
		compatible = "sm5450";
		reg = <0x6F>;
		pinctrl-names = "default";
		pinctrl-0 = <&sm5450_main_irq_pins_default>;
		intr_gpio_num = <45>; /* direct defined GPIO num */
		intr_gpio = <&pio 45 0>;
		adc_channel_vbat_sys = <14>;
		sense_r_config = <50>;
		sense_r_actual = <20>;
		fcp_support = <1>;
		scp_support = <1>;
		lvc_ibat_regulator = <12200>;
		lvc_vbat_regulator = <4500>;
		lvc_vout_regulator = <4550>;
		lvc_ibus_regulator = <5600>;
		sc_ibat_regulator = <12200>;
		sc_vbat_regulator = <4500>;
		sc_vout_regulator = <4550>;
		sc_ibus_regulator = <5600>;
		lvc_vusb2vbus_drop_en = <1>;
		sc_vusb2vbus_drop_en = <1>;
		bat_comp = <30>;
		vclamp = <90>;
		r_coul_mohm = <5>;
		switching_frequency = <675>;
		status = "ok";
	};
};

&i2c0 {
	sm5450@6F {
		compatible = "sm5450";
		reg = <0x6F>;
		pinctrl-names = "default";
		pinctrl-0 = <&sm5450_aux_irq_pins_default>;
		intr_gpio_num = <43>; /* direct defined GPIO num */
		intr_gpio = <&pio 43 0>;
		adc_channel_vbat_sys = <14>;
		sense_r_config = <50>;
		sense_r_actual = <20>;
		fcp_support = <0>;
		scp_support = <0>;
		lvc_ibat_regulator = <12200>;
		lvc_vbat_regulator = <4500>;
		lvc_vout_regulator = <4550>;
		lvc_ibus_regulator = <5600>;
		sc_ibat_regulator = <12200>;
		sc_vbat_regulator = <4500>;
		sc_vout_regulator = <4550>;
		sc_ibus_regulator = <5600>;
		lvc_vusb2vbus_drop_en = <1>;
		sc_vusb2vbus_drop_en = <1>;
		bat_comp = <30>;
		vclamp = <90>;
		r_coul_mohm = <5>;
		ic_role = <1>;
		switching_frequency = <675>;
		status = "ok";
	};
};

&direct_charger {
	volt_para =
		"4250", "5200", "4800",
		"4350", "4800", "2800",
		"4450", "2800", "1200";
	temp_para =
		"-32767", "10", "0",
		"10", "45", "5200",
		"45", "32767", "0";
	status = "ok";
};

&direct_charger_sc {
	support_multi_ic = <1>;
	rt_test_para =
		/* first:sc second:mainsc third:auxsc */
		"3000", "105",
		"3000", "105",
		"3000", "105";
};

/* storage nano sd GPIO standardization */
&msdc1 {
	pinctrl-names = "default",
		"sd_clk_function0_np",
		"sd_clk_function1_np",
		"sd_cmd_pu",
		"sd_cmd_np",
		"sd_data0_pu",
		"sd_data0_np",
		"sd_data1_pu",

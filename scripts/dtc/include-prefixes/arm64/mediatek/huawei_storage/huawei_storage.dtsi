		"sd_data2_pu",
		"sd_data2_np",
		"sd_data3_pu",
		"sd_data3_np",
		"sim_clk_function0_np",
		"sim_clk_function1_pd",
		"sim_rst_function0_np",
		"sim_rst_function1_pd",
		"sim_data_function0_np",
		"sim_data_function1_pu";
	pinctrl-0 = <&sd_default>;
	pinctrl-1 = <&sd_clk_function0_np>;
	pinctrl-2 = <&sd_clk_function1_np>;
	pinctrl-3 = <&sd_cmd_pu>;
	pinctrl-4 = <&sd_cmd_np>;
	pinctrl-5 = <&sd_data0_up>;
	pinctrl-6 = <&sd_data0_np>;
	pinctrl-7 = <&sd_data1_up>;
	pinctrl-8 = <&sd_data1_np>;
	pinctrl-9 = <&sd_data2_up>;
	pinctrl-10 = <&sd_data2_np>;
	pinctrl-11 = <&sd_data3_up>;
	pinctrl-12 = <&sd_data3_np>;
	pinctrl-13 = <&sim_clk_function0_np>;
	pinctrl-14 = <&sim_clk_function1_pd>;
	pinctrl-15 = <&sim_rst_function0_np>;
	pinctrl-16 = <&sim_rst_function1_pd>;
	pinctrl-17 = <&sim_data_function0_np>;
	pinctrl-18 = <&sim_data_function1_pu>;
	status = "okay";
};


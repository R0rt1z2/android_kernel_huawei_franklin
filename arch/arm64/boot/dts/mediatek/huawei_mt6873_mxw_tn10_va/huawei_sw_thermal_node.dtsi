/* shell thermal config and algorithm AR000AFR67 */
&hisi_shell {
	compatible = "hisi,shell-temp";
	interval = <5000>;
	bias = <454184>;
	count = <7>;
	tsensor_para = "10000","100000","0";
	ntc_para = "2000","80000","-20000";
	status = "ok";
	sensors {
		sensor0 {
			type = "msm_therm";
			coef = "3542","631","-40","2037","1431","4652","14225";
		};
		sensor1 {
			type = "pa_therm0";
			coef = "5750","4450","1341","769","-480","1259","16503";
		};
		sensor2 {
			type = "CHG_IC_THERM";
			coef = "-10738","-125","824","1016","1","-2969","-913";
		};
		sensor3 {
			type = "COOL_THERM";
			coef = "-1396","224","597","2844","1827","-333","2976";
		};
		sensor4 {
			type = "battery";
			coef = "-7148000","-3324000","-1047000","2151000","7680000","13382000","23089000";
		};
	};
};


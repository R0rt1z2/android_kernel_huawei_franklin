/* shell thermal config and algorithm AR000AFR67 */
&hisi_shell {
	compatible = "hisi,shell-temp";
	interval = <5000>;
	bias = <691168>;
	count = <7>;
	tsensor_para = "10000","100000","0";
	ntc_para = "2000","80000","-20000";
	status = "ok";
	sensors {
		sensor0 {
			type = "msm_therm";
			coef = "7568","4953","2905","3305","1926","2913","20491";
		};
		sensor1 {
			type = "pa_therm0";
			coef = "-7698","4800","4902","4851","5032","4284","30575";
		};
		sensor2 {
			type = "CHG_IC_THERM";
			coef = "-8371","270","379","-1385","-2165","-3761","-3796";
		};
		sensor3 {
			type = "pmic_xo_therm";
			coef = "-5851","-1991","-627","1486","2074","1541","5834";
		};
		sensor4 {
			type = "battery";
			coef = "-17108000","-2529000","-408000","-2717000","-6018000","923000","31749000";
		};
	};
};


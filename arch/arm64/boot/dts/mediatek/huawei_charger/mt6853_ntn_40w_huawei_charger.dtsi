	battery_cv = <4430000>;
	iterm = <150000>;
	vbus_max_limit = <11500>;
	jeita_temp_above_t4_cv = <4100000>;
	jeita_temp_t3_to_t4_cv = <4100000>;
	jeita_temp_t2_to_t3_cv = <4430000>;
	jeita_temp_t1_to_t2_cv = <4430000>;
